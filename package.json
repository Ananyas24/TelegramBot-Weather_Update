{
  "name": "nestjs-telegram-bot",
  "version": "1.0.0",
  "description": "A NestJS Telegram bot with Google login for admin panel",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^1.1.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "axios": "^0.21.1",
    "node-fetch": "^2.6.1",
    "node-schedule": "^2.0.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/node": "^14.14.31",
    "@types/passport": "^1.0.11",
    "@types/passport-google-oauth20": "^2.0.10",
    "@types/node-fetch": "^2.5.8",
    "@types/node-schedule": "^1.3.0",
    "@types/estree": "^0.0.39",
    "@types/eslint": "^6.8.1",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
